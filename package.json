{
  "name": "story-squad-be",
  "version": "2.0.1",
  "private": true,
  "author": "Story Squad Developers - Lambda School",
  "description": "The service services for the Story Squad application",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn prisma generate",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prestart": "yarn migrate:up",
    "start": "node dist/main",
    "dev": "pipenv run nest start --watch",
    "debug": "pipenv run nest start --debug --watch",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "eslint  --fix '{src, test}/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest.config.js",
    "test:e2e:watch": "jest --config ./test/jest.config.js --watch",
    "nest": "nest",
    "prisma": "prisma2",
    "migrate:up": "prisma2 migrate up --experimental",
    "migrate:down": "prisma2 migrate down --experimental",
    "migrate:save": "source .env && prisma2 migrate save --experimental",
    "postmigrate:save": "source .env && prisma2 generate",
    "migrate:up:dev": "source .env && yarn migrate:up",
    "migrate:down:dev": "source .env && yarn migrate:down",
    "migrate:up:test": "source .env.test && yarn migrate:up",
    "migrate:down:test": "source .env.test && yarn migrate:down",
    "seed": "ts-node -r tsconfig-paths/register ./prisma/seed"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.5",
    "@nestjs/config": "^0.1.0",
    "@nestjs/core": "^6.11.5",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-fastify": "^6.11.5",
    "@prisma/client": "^2.0.0-preview021",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "helmet": "^3.21.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "python-shell": "^1.0.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.1",
    "rxjs": "^6.5.4",
    "secure-random-password": "^0.2.1",
    "stripe": "^8.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.1",
    "@nestjs/platform-express": "^6.11.5",
    "@nestjs/schematics": "^6.9.3",
    "@nestjs/testing": "^6.11.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/secure-random-password": "^0.1.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "prisma2": "^2.0.0-preview021",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.5"
  }
}
