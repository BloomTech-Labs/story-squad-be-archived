{
  "name": "story-squad-be",
  "version": "0.0.1",
  "private": true,
  "author": "Story Squad Developers - Lambda School",
  "description": "The service services for the Story Squad application",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn prisma generate",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prestart": "yarn migrate:up",
    "start": "node dist/main",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "eslint  --fix '{src, test}/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "nest": "nest",
    "prisma": "prisma2",
    "migrate:save": "prisma2 migrate save --experimental",
    "migrate:up": "prisma2 migrate up --experimental",
    "migrate:down": "prisma2 migrate down --experimental",
    "seed": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/config": "^0.1.0",
    "@nestjs/core": "^6.10.14",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.10.14",
    "@prisma/client": "^2.0.0-preview020.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "dotenv": "^8.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "secure-random-password": "^0.2.1",
    "stripe": "^8.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.10.14",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "@types/passport-jwt": "^3.0.3",
    "@types/secure-random-password": "^0.1.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "prisma2": "^2.0.0-preview020.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  }
}
